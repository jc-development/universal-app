{
    "plugins": [
        ["dynamic-import-webpack"],
        ["syntax-dynamic-import"],
        ["transform-object-rest-spread"],
        ["transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }]
    ],
    "presets": [
        "react",
        [
            "env",
            {
                "targets": {
                    "browsers": ["last 2 versions", "ie >= 11"],
                },
            }
        ]
    ]
}
